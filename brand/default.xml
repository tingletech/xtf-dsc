<?xml version="1.0"?>

<brand owner="eric.satzman@ucop.edu" href="default.xml" multi-use="default-breadcrumb">
  <links>
    <!-- link to the entry .css file; CHANGE THIS TO YOUR PROJECT'S ENTRY .css FILE, which includes the default.css -->
    <link rel="stylesheet" href="http://content-dev.cdlib.org/default/css/default.css" type="text/css" xmlns="http://www.w3.org/1999/xhtml" />
	<link xmlns="http://www.w3.org/1999/xhtml" rel="stylesheet" href="http://content-dev.cdlib.org/default/css/print.css" type="text/css" media="print" />
    <!-- link to the .js files -->
    <script src="http://content-dev.cdlib.org/default/js/nifty.js" type="text/javascript" xmlns="http://www.w3.org/1999/xhtml" />
    <script src="http://content-dev.cdlib.org/default/js/default.js" type="text/javascript" xmlns="http://www.w3.org/1999/xhtml" />
    <!-- CHANGE THE LINK TO YOUR PROJECT'S .js FILE -->
    <script src="http://yourproject/project.js" type="text/javascript" xmlns="http://www.w3.org/1999/xhtml" /> 
  </links>
  
  <header>
    <a href="http://yourproject/home.html"><img src="http://content-dev.cdlib.org/default/images/header.gif" /></a>
  </header>
  
  <header.dynaxml.header>
    <!-- links should be target="_top" for the framed pages -->
    <a href="http://yourproject/home.html" target="_top"><img src="http://content-dev.cdlib.org/default/images/header.gif" border="0"/></a>
  </header.dynaxml.header>
  
  <footer>
    <!-- links should be target="_top" for the framed pages -->
    <div id="footer">
    <a href="http://yourproject/home.html" target="_top"><img src="http://content-dev.cdlib.org/default/images/footer.gif" border="0" /></a>
    </div>
  </footer>
 
  <!-- navigation arrows on the left-side of the object views --> 
  <arrow.up src="http://content-dev.cdlib.org/default/images/blue_arrow_up.gif" width="9" height="11" border="0" />
  <arrow.dn src="http://content-dev.cdlib.org/default/images/blue_arrow_dn.gif" width="11" height="9" border="0" />

  <!-- the icon that says "Printable Version" -->
  <print.img src="http://content-dev.cdlib.org/default/images/printable_version.gif" width="110" height="16" />
  
  <!-- the search button  for searching within a TEI -->
  <search.img>
<input src="http://content-dev.cdlib.org/default/images/search.gif" class="search-button" alt="Search" title="Search" type="image" />
  </search.img>

  <!-- previous and next buttons for jumping from hit to hit after searching within a TEI -->
  <in.prev src="http://content-dev.cdlib.org/default/images/b_inprev.gif" border="0" alt="previous hit" />
  <in.next src="http://content-dev.cdlib.org/default/images/b_innext.gif" border="0" alt="next hit" />

</brand>
